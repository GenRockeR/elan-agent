# Remove freeradius start script as we use upstart
if ! dpkg-divert --list "ea-authentication" | grep -F "/etc/init.d/freeradius.diverted-ea-authentication"
then
        dpkg-divert --package "ea-authentication" --add --rename \
                --divert "/etc/init.d/freeradius.diverted-ea-authentication" "/etc/init.d/freeradius"
fi