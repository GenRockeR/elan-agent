description "Access Control"
 
start on started redis

env PYTHONPATH=/origin/lib/python

respawn

pre-start script

  if [ ! -e /etc/network/interfaces.d/ea-network ]
  then
    python3 -c 'import origin.network; origin.network.NetworkConfiguration().apply_configuration()'
  fi
  
  modprobe br_netfilter
  
  nft add table bridge origin
  nft add table ip origin
  nft add table ip6 origin
  nft -f /origin/network/nftables.sets || true
  nft -f /origin/network/nftables.chains
  
  #echo nfnetlink_log > /proc/sys/net/netfilter/nf_log/7

end script

exec  /origin/bin/access-control-configurator
