cp -p /usr/bin/python %{ORIGIN_PREFIX}/captive-portal/bin/
if [ ! -e %{ORIGIN_PREFIX}/captive-portal/bin/python2 ]
then
  ln -s python %{ORIGIN_PREFIX}/captive-portal/bin/python2
fi
if [ ! -e %{ORIGIN_PREFIX}/captive-portal/bin/python2.7 ]
then
  ln -s python %{ORIGIN_PREFIX}/captive-portal/bin/python2.7
fi

reload captive-portal || true