python auth_provider_failed {
        module = origin.freeradius

        mod_authorize = ${.module}
        func_authorize = AuthenticationProviderFailed

        mod_authenticate = ${.module}
        func_authenticate = AuthenticationProviderFailed

        mod_preacct = ${.module}
        func_preacct = AuthenticationProviderFailed

        mod_accounting = ${.module}
        func_accounting = AuthenticationProviderFailed

        mod_pre_proxy = ${.module}
        func_pre_proxy = AuthenticationProviderFailed

        mod_post_proxy = ${.module}
        func_post_proxy = AuthenticationProviderFailed

        mod_post_auth = ${.module}
        func_post_auth = AuthenticationProviderFailed

}

